{
  // Required
  "manifest_version": 2,
  "name": "GitHub Plus",
  "version": "0.1.0",

  // Recommended
  // "default_locale": "en",
  "description": "Adds extra fields to GitHub issues.",

  // Pick one (or none)
  // "browser_action": {...},
  // "page_action": {...},

