    //https://github.com/ifit/ifit/issues/1039
    "matches": ["https://github.com/*/*/issues/*", "https://github.com/ifit/ifit/issues/1039"],
    "js": ["js/_inject.js"],
    "css": ["css/jquery-ui-1.10.3.custom.css"],
    "run_at": "document_end"
  }],
